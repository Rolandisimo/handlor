{
    "include": [
      "lib/**/*.ts",
      "./defs.d.ts"
    ],
    "compilerOptions": {
      "allowSyntheticDefaultImports": false,
      "target": "es5",
      "module": "commonjs",
      "declaration": true,
      "outDir": "./dist",
      "strict": true,
      "downlevelIteration": true,
      "importHelpers": true,
      "sourceMap": true,
      "moduleResolution": "node",
      "allowJs": false,
      "checkJs": false,
      "lib": [
        "dom",
        "es2015",
        "es2016",
        "es2017",
        "esnext"
      ],
      "forceConsistentCasingInFileNames": true,
      "experimentalDecorators": true,
      "noImplicitAny": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "skipLibCheck": true,
      "strictNullChecks": false,
      "suppressImplicitAnyIndexErrors": true,
    }
  }
  